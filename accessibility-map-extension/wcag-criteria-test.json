{
    "Versteckte Elemente": {
        "selectors": "",
        "color": "",
        "type": "hiddenToggle",
        "enabled": false,
        "showHidden": false
    },
    "Struktur": {
        "selectors": "header, [role=banner], aside, [role=complementary], nav, [role=navigation], main, [role=main], footer, [role=contentinfo]",
        "color": "hsla(180, 100%, 50%, 0.85)",
        "type": "mixed",
        "enabled": false,
        "showElement": true
    },
    "1.3.1 Überschriften": {
        "selectors": "h1, h2, h3, h4, h5, h6, [role=heading]",
        "color": "hsla(180, 100%, 50%, 0.85)",
        "type": "element",
        "enabled": false
    },
    "1.3.1 Listen": {
        "selectors": "ul, ol, dl, li, menu",
        "color": "hsla(30, 100%, 50%, 0.85)",
        "type": "element",
        "enabled": false
    },
    "1.3.1 Zitate": {
        "selectors": "blockquote, q, [role=blockquote]",
        "color": "hsla(300, 100%, 50%, 0.85)",
        "type": "element",
        "enabled": false
    },
    "1.3.1 Inhalte gegliedert": {
        "selectors": "p, em, strong",
        "color": "hsla(190, 50%, 60%, 0.85)",
        "type": "element",
        "enabled": true,
        "showElement": true,
        "lines": {
            "start": ["title"],
            "end": "id"
        }
    },
    "1.3.1 Tabellen": {
        "selectors": "table, [role=table], th, td, caption",
        "color": "hsla(0, 100%, 50%, 0.85)",
        "type": "mixed",
        "enabled": false
    },
    "2.4.7 Linktexte": {
        "selectors": "a[href], [role=link]",
        "color": "hsla(0, 100%, 50%, 0.85)",
        "type": "mixed",
        "enabled": false
    },
    "4.1.2 Name, Rolle, Wert (roles)": {
        "selectors": "[role]",
        "color": "hsla(0, 100%, 50%, 0.85)",
        "type": "attribute",
        "enabled": false
    },
    "ARIA Texte": {
        "selectors": "[aria-label], [aria-describedby], [aria-labelledby]",
        "color": "hsla(0, 100%, 70%, 0.85)",
        "type": "attribute",
        "enabled": false
    },
    "4.3.1 Statusmeldungen (Live Regionen)": {
        "selectors": "[aria-live], [aria-atomic], [aria-relevant], [aria-busy]",
        "color": "hsla(0, 100%, 70%, 0.85)",
        "type": "attribute",
        "enabled": false
    },
    "1.1.1 Non-Text Alternativen": {
        "selectors": "[alt]",
        "color": "hsla(0, 100%, 30%, 0.85)",
        "type": "attribute",
        "enabled": false
    },
    "2.1.1 Tab-Reihenfolge": {
        "selectors": "a[href], button, input, textarea, select, [tabindex]:not([tabindex=\"-1\"])",
        "color": "hsla(120, 100%, 30%, 0.85)",
        "type": "element",
        "enabled": false
    },
    "2.4.7 Fokus deutlich": {
        "selectors": "a[href], button, input, textarea, select, [tabindex]:not([tabindex=\"-1\"])",
        "color": "hsla(280, 100%, 60%, 0.85)",
        "type": "focus",
        "enabled": true,
        "highlightCurrent": true,
        "showTabIndex": true
    },
    "3.3.2 Beschriftungen (über id)": {
        "selectors": "[aria-labelledby], [aria-describedby], [for]",
        "color": "hsla(0, 100%, 70%, 0.85)",
        "type": "attribute",
        "enabled": false,
        "lines": {
            "start": ["[aria-labelledby]", "[aria-describedby]", "[for]"],
            "end": "id"
        }
    },
    "1.4.3 Kontraste": {
        "selectors": "",
        "color": "",
        "type": "contrast",
        "enabled": false,
        "showElement": false
    }
}
